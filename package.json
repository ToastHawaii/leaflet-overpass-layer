{
  "name": "leaflet-overpass-layer",
  "description": "This is an simple way to bring OSM data as POI overlay on your leaflet Map",
  "version": "3.0.0",
  "homepage": "https://github.com/ToastHawaii/leaflet-overpass-layer/",
  "repository": "https://github.com/ToastHawaii/leaflet-overpass-layer/",
  "bugs": "https://github.com/ToastHawaii/leaflet-overpass-layer/issues",
  "author": [
    "Karsten Hinz",
    "Guillaume Amat",
    "ToastHawaii"
  ],
  "license": "MIT",
  "keywords": [
    "leaflet",
    "overpass",
    "layer",
    "marker",
    "osm",
    "openstreetmap"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --progress && tsc --build ./src/tsconfig.json",
    "watch": "webpack -w --progress",
    "precommit": "lint-staged"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ],
    "src/**/*.{js,jsx,json}": [
      "eslint",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/runtime": "7.17.9",
    "@types/md5": "2.3.2",
    "@types/leaflet": "1.5.17",
    "idb": "7.0.1",
    "md5": "2.3.0",
    "js-clipper": "1.0.1",
    "leaflet": "1.7.1"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "7.17.3",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-typescript": "7.16.7",
    "babel-loader": "8.2.5",
    "copy-webpack-plugin": "10.2.4",
    "css-loader": "6.7.1",
    "file-loader": "6.2.0",
    "raw-loader": "4.0.2",
    "source-map-loader": "3.0.1",
    "style-loader": "3.3.1",
    "typescript": "4.6.3",
    "webpack": "5.72.0",
    "webpack-cli": "4.9.2"
  },
  "resolutions": {
    "terser": "3.14.1"
  }
}
